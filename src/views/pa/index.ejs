            <ul>
                <li onclick="{select_type}" value="fa">亲子少儿</li>
                <li onclick="{select_type}" value="of">户外健身</li>
                <li onclick="{select_type}" value="tn">周边游</li>
                <li onclick="{select_type}" value="pt">聚会派对</li>
                <li onclick="{select_type}" value="ex">展览展会</li>
                <li onclick="{select_type}" value="tb">团队建设</li>
                <li onclick="{select_type}" value="tr">旅行</li>
                <li onclick="{select_type}" value="oa">其他活动</li>
            </ul>
            <div class="clear"></div>
            <div class="label_button">
                <div class="label_button_div">
                    <button class="confirm_but1 fl" onclick="{setAllType}"><img src="../public/images/but_s1.png" class="vertical" /> <span class="vertical" >全部</span></button>
                    <button class="confirm_but2 fr" onclick="{typeSelectObj}"><img src="../public/images/but_s2.png" class="vertical"/> <span class="vertical" >筛选</span></button>
                </div>
            </div>
        </div>
    </div>
